  "lint-staged": {
    "**/*.{ts,js,jsx,tsx}": [
      "npm run lint:es",
      "prettier --write"
    ],
    "*.(css|scss)": [
      "npm run stylelint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
